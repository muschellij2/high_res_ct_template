	number = {1},
	journaltitle = {Journal of the Neurological Sciences},
	shortjournal = {Journal of the Neurological Sciences},
	author = {Jang, Sung Ho and Ahn, Sang Ho and Sakong, Joon and Byun, Woo Mok and Choi, Byung Yun and Chang, Chul Hoon and Bai, Daiseg and Son, Su Min},
	urldate = {2014-11-12},
	date = {2010-03-15},
	note = {00037},
	keywords = {Brian mapping, Diffusion tensor imaging, Intracerebral hemorrhage, Motor recovery, Stroke, Transcranial magnetic stimulation},
	file = {ScienceDirect Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/TF84TIBJ/Jang et al. - 2010 - Comparison of TMS and DTT for predicting motor out.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/JAG5Z3U8/S0022510X0900937X.html:text/html}
}

@article{kothari_abcs_1996,
	title = {The {ABCs} of Measuring Intracerebral Hemorrhage Volumes},
	volume = {27},
	issn = {0039-2499, 1524-4628},
	url = {http://stroke.ahajournals.org/content/27/8/1304},
	doi = {10.1161/01.STR.27.8.1304},
	abstract = {Background and Purpose Hemorrhage volume is a powerful predictor of 30-day mortality after spontaneous intracerebral hemorrhage ({ICH}). We compared a bedside method of measuring {CT} {ICH} volume with measurements made by computer-assisted planimetric image analysis.
Methods The formula {ABC}/2 was used, where A is the greatest hemorrhage diameter by {CT}, B is the diameter 90° to A, and C is the approximate number of {CT} slices with hemorrhage multiplied by the slice thickness.
Results The {ICH} volumes for 118 patients were evaluated in a mean of 38 seconds and correlated with planimetric measurements (R2=.96). Interrater and intrarater reliability were excellent, with an intraclass correlation of .99 for both.
Conclusions We conclude that {ICH} volume can be accurately estimated in less than 1 minute with the simple formula {ABC}/2.},
