	keywords = {Adult, Aged, Aged, 80 and over, Case-Control Studies, cerebral hemorrhage, Cohort Studies, Databases, Factual, Female, Follow-Up Studies, General Practice, Health Status, Humans, Longitudinal Studies, Male, Middle Aged, Risk Factors, Stroke, Young Adult},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/48QA3C9X/Gonz\`alez-P\'erez et al. - 2013 - Mortality after hemorrhagic stroke Data from gener.pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/M2CWWZQU/559.html:text/html;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/A3CIAVXV/559.html:text/html}
