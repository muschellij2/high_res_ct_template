	urldate = {2014-11-12},
	date = {2007-01-01},
	langid = {english},
	note = {00061 {PMID}: 17122428},
	keywords = {infarcts, Magnetic Resonance Imaging, models, Outcome, statistical, Stroke},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/AWVPIHNT/Menezes et al. - 2007 - The Real Estate Factor Quantifying the Impact of I.pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/BD82269W/194.html:text/html}
