	langid = {english},
	note = {00005},
	keywords = {Brain, Computer Imaging, Vision, Pattern Recognition and Graphics, Computer Science, general, {CT}, Health Informatics, Hemorrhage, Imaging / Radiology, Level sets, Segmentation, surgery},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/E226UASC/Prakash et al. - 2012 - Segmentation and quantification of intra-ventricul.pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/6DQNG9R4/s11548-012-0670-0.html:text/html}
