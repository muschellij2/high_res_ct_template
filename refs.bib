	keywords = {acute ischemic stroke, acute stroke, adaptive window settings determination, automatic window settings determination, autowindowing, auto-windowing, biomedical {MRI}, Brain, brain {DWI}, cerebral ischemic changes, coarse lesion segmentation, contrast enhancement, contrast-noise ratio based computation, Diffusion weighted imaging, diffusion weighted {MRI}, image contrast enhancement, image segmentation, ischemic lesions, ischemic stroke lesions, Magnetic Resonance Imaging, medical disorders, medical image processing, mirror region of interest analysis, optimal window parameters, perception study, statistical analysis, statistical t-test},
	file = {IEEE Xplore Abstract Record:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/DC7HWDFH/abs_all.html:text/html;Mujumdar et al. - 2013 - Auto-windowing of ischemic stroke lesions in diffu.pdf:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/8V5AVZ27/Mujumdar et al. - 2013 - Auto-windowing of ischemic stroke lesions in diffu.pdf:application/pdf}
