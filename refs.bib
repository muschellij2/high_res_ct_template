	number = {7},
	journaltitle = {Stroke},
	shortjournal = {Stroke},
	author = {Broderick, J. P. and Brott, T. G. and Duldner, J. E. and Tomsick, T. and Huster, G.},
	urldate = {2014-04-24},
	date = {1993-07-01},
	langid = {english},
	pmid = {8322400},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/PTSXEFCU/Broderick et al. - 1993 - Volume of intracerebral hemorrhage. A powerful and.pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/ACXEM87E/987.html:text/html}
}

@article{brott_measurements_1989,
	title = {Measurements of acute cerebral infarction: a clinical examination scale.},
	volume = {20},
	issn = {0039-2499, 1524-4628},
