}

@article{johnson_consistent_2002,
	title = {Consistent landmark and intensity-based image registration},
	volume = {21},
	issn = {0278-0062},
	doi = {10.1109/TMI.2002.1009381},
	abstract = {Two new consistent image registration algorithms are presented: one is based on matching corresponding landmarks and the other is based on matching both landmark and intensity information. The consistent landmark and intensity registration algorithm produces good correspondences between images near landmark locations by matching corresponding landmarks and away from landmark locations by matching the image intensities. In contrast to similar unidirectional algorithms, these new consistent algorithms jointly estimate the forward and reverse transformation between two images while minimizing the inverse consistency error-the error between the forward (reverse) transformation and the inverse of the the reverse (forward) transformation. This reduces the ambiguous correspondence between the forward and reverse transformations associated with large inverse consistency errors. In both algorithms a thin-plate spline ({TPS}) model is used to regularize the estimated transformations. Two-dimensional (2-D) examples are presented that show the inverse consistency error produced by the traditional unidirectional landmark {TPS} algorithm can be relatively large and that this error is minimized using the consistent landmark algorithm. Results using 2-D magnetic resonance imaging data are presented that demonstrate that using landmark and intensity information together produce better correspondence between medical images than using either landmarks or intensity information alone.},
	pages = {450--461},
	number = {5},
	journaltitle = {{IEEE} Transactions on Medical Imaging},
	author = {Johnson, H. J. and Christensen, G.E.},
	date = {2002-05},
	keywords = {2-D magnetic resonance imaging data, Algorithms, ambiguous correspondence, Anisotropic magnetoresistance, Biomedical imaging, biomedical {MRI}, Brain, Cities and towns, Computer Simulation, consistent landmark registration, deformable templates, estimated transformations regularization, forward transformation, Humans, image enhancement, Image Interpretation, Computer-Assisted, Image registration, intensity-based image registration, interpolation, inverse consistency error minimization, inverse transformation, Magnetic Resonance Imaging, medical diagnostic imaging, medical image processing, Reproducibility of Results, reverse transformation, Sensitivity and Specificity, Spline, splines (mathematics), Subtraction Technique, thin-plate spline model, Two dimensional displays, unidirectional landmark algorithm},
	file = {IEEE Xplore Abstract Record:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/A2DMCIKP/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/ENFSZ3DH/Johnson and Christensen - 2002 - Consistent landmark and intensity-based image regi.pdf:application/pdf}
}

@article{jongen_construction_2004,
	title = {Construction and evaluation of an average {CT} brain image for inter-subject registration},
	volume = {34},
	issn = {0010-4825},
	url = {http://www.sciencedirect.com/science/article/pii/S001048250300101X},
	doi = {10.1016/j.compbiomed.2003.10.003},
	abstract = {An average {CT} brain image is constructed to serve as reference frame for inter-subject registration. A set of 96 clinical {CT} images is used. Registration includes translation, rotation, and anisotropic scaling. A temporary average based on a subset of 32 images is constructed. This image is used as reference for the iterative construction of the average {CT} image. This approach is computationally efficient and results in a consistent registration of the 96 images. Registration of new images to the average {CT} is more consistent than registration to a single {CT} image. The use of the average {CT} image is illustrated.},
